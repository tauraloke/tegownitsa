{
  "name": "tegownitsa",
  "version": "0.14.1",
  "private": true,
  "description": "Yet another image tag manager",
  "author": {
    "name": "Tauraloke",
    "email": "tauraloke@gmail.com",
    "url": "https://github.com/tauraloke"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "dist": "npm run electron:build",
    "electron:build": "npm run jsonize && vue-cli-service electron:build",
    "electron:serve": "npm run jsonize && vue-cli-service electron:serve",
    "i18n:report": "vue-cli-service i18n:report --src \"./src/**/*.?(js|vue)\" --locales \"./src/locales/**/*.json\"",
    "jest": "npx jest .",
    "jsonize": "npm run jsonize:api && npm run jsonize:store_defaults",
    "jsonize:api": "node ./src/scripts/pack_api.js",
    "jsonize:store_defaults": "node ./src/scripts/pack_store_defaults.js",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "release": "np",
    "dev:clean": "rm -rf ./node_modules/electron/dist/storage/ && rm ./db.sqlite3",
    "start": "npm run electron:serve",
    "test": "npm run lint && npm run jest"
  },
  "main": "background.js",
  "dependencies": {
    "@mdi/font": "7.0.96",
    "@tensorflow/tfjs-node": "^3.20.0",
    "core-js": "^3.25.3",
    "electron-debug": "^3.2.0",
    "electron-dl": "^3.3.1",
    "electron-store": "^8.1.0",
    "electron-unhandled": "^4.0.1",
    "electron-updater": "^5.2.1",
    "electron-util": "^0.17.2",
    "exif-parser": "^0.1.12",
    "install": "^0.13.0",
    "iqdb-client": "^2.0.0",
    "npm": "^8.19.2",
    "roboto-fontface": "*",
    "sharp": "^0.31.0",
    "sharp-phash": "^2.0.0",
    "sqlite3": "^5.1.1",
    "tesseract.js": "^3.0.3",
    "vue": "^3.2.39",
    "vue-i18n": "^9.2.2",
    "vuetify": "^3.0.0-beta.11",
    "webfontloader": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/eslint-parser": "^7.19.1",
    "@intlify/vue-i18n-loader": "^4.2.0",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "electron": "^20.2.0",
    "electron-builder": "^23.3.3",
    "electron-devtools-installer": "^3.1.0",
    "eslint": "^8.24.0",
    "eslint-plugin-unicorn": "^43.0.2",
    "eslint-plugin-vue": "=8.7.1",
    "jest": "^29.0.3",
    "np": "^7.5.0",
    "prettier": "2.7.1",
    "vue-cli-plugin-electron-builder": "~2.1.1",
    "vue-cli-plugin-i18n": "~2.3.1",
    "vue-cli-plugin-vuetify": "~2.5.8",
    "webpack-plugin-vuetify": "^2.0.0-alpha.13",
    "xo": "^0.52.3"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "license": "MIT",
  "np": {
    "publish": false,
    "releaseDraft": false
  },
  "overrides": {
    "cheerio": "1.0.0-rc.10",
    "parse5-htmlparser2-tree-adapter": "^6.0.0",
    "vue-cli-plugin-electron-builder": {
      "electron-builder": "^23.0.6"
    }
  },
  "productName": "Tegownitsa",
  "repository": "tauraloke/tegownitsa",
  "xo": {
    "envs": [
      "node",
      "browser"
    ]
  }
}
